# Check-a-no-is-a-prime-using-function-
#include <stdio.h>

int isPrime(int num) {
    if (num <= 1) return 0;
    for (int i = 2; i <= num / 2; i++) {
        if (num % i == 0) return 0;
    }
    return 1;
}

int main() {
    int num = 11;
    if (isPrime(num))
        printf("%d is a Prime Number\n", num);
    else
        printf("%d is not a Prime Number\n", num);
    return 0;
}
